<div class="container">
  <div class="row">
    <div class="col">
      <h1 class="mb-5"><%= current_company.name %></h1>
    </div>
  </div>
  <% @professions_users.each do |profession_users| %>
    <div class="row my-4" style="border-bottom: 1px #eee solid">
      <h4 class="mr-3"><%= profession_users.first.profession.name %></h4>
      <div class="col">
        <%= render partial: "companies/profession_user", collection: profession_users, as: :user %>
      </div>
    </div>
  <% end %>
</div>